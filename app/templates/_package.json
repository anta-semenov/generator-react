{
  "name": "<%= appName %>",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "NODE_ENV=dev node server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "deploy": "node build.js"
  },
  "dependencies": {},
  "devDependencies": {},
  "keywords": [],
  "author": "Anton Semenov",
  "license": "MIT",
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "stage-1",
      "react"
    ]
  },
  "eslintConfig": {
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    },
    "env": {
      "browser": true,
      "node": true,
      "jest": true,
      "mocha": true
    },
    "parser": "babel-eslint",
    "rules": {
      "quotes": [
        1,
        "single"
      ],
      "strict": [
        2,
        "never"
      ],
      "semi": [
        0,
        "always"
      ],
      "no-unused-vars": [
        1
      ],
      "react/jsx-uses-react": 2,
      "react/jsx-uses-vars": 2,
      "react/react-in-jsx-scope": 2,
      "no-alert": 0
    },
    "extends": "eslint:recommended",
    "plugins": [
      "react"
    ]
  }
}
